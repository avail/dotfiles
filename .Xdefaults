Xft.dpi:                    96
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                false
Xft.hintstyle:              hintslight

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
URxvt.loginShell:           true
URxvt.saveLines:            65535
URxvt*saveLines: 65535
URxvt.internalBorder:       5
URxvt.lineSpace:            0

URxvt.allow_bold:           true
URxvt.font: xft:Mensch:pixelsize=15
URxvt.boldFont: xft:Mensch:pixelsize=15
URxvt*letterSpace: -1

URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

URxvt.perl-ext-common:      default,matcher,selection-to-clipboard
URxvt.matcher.button:       1
URxvt.urlLauncher:          firefox

URxvt.cursorBlink:          true
URxvt.cursorColor:          #FFCC66
URxvt.cursorUnderline:      false

URxvt.pointerBlank:         true

URxvt*background: #212733
URxvt*foreground: #D7D5CC
*background: #212733
*foreground: #D7D5CC

! black
URxvt*color0:  #222946
! dark grey
URxvt*color8:  #8C8B89

! red
URxvt*color1:  #B32323
! bright red
URxvt*color9:  #DC2C2C

! green
URxvt*color2:  #1AAA13
! bright green
URxvt*color10: #5DCE33

! yellow
URxvt*color3:  #FF9F3E
! bright yellow
URxvt*color11: #E6CB29

! blue
URxvt*color4:  #4957F3
! bright blue
URxvt*color12: #4D79FF

! magenta
URxvt*color5:	#BF3E94
! bright magenta
URxvt*color13:	#B641CC

! cyan
URxvt*color6:  #09A5B3
! bright cyan
URxvt*color14: #3CB6F3

! light grey
URxvt*color7:  #AEAEAE
! white
URxvt*color15: #DBDDE4